{
  "name": "@wmfs/cardscript-schema",
  "description": "Contains a JSON Schema for Cardscript, along with a validation utility.",
  "version": "0.0.0-semantically-released",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "dottie": "2.0.2",
    "jsonschema": "1.2.7",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "semantic-release": "17.3.0",
    "standard": "15.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@wmfs/cardscript-examples": "^1.16.0"
